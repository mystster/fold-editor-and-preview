{
  "compilerOptions": {
    // "target": "es5",
    "target": "ES2018",
    "lib": ["dom", "dom.iterable", "esnext"],
    "outDir": "lib",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "newLine": "lf"
  },
  "include": ["src"]
}
